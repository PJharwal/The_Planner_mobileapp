{
  "expo": {
    "name": "Student Study App",
    "slug": "student-study-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "student-study-app",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.studyapp.student",
      "infoPlist": {
        "NSHealthShareUsageDescription": "Tasky uses your activity and sleep data to recommend better focus sessions and prevent burnout. Your data never leaves your device.",
        "NSHealthUpdateUsageDescription": "Tasky uses your activity and sleep data to recommend better focus sessions and prevent burnout. Your data never leaves your device.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f172a"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.studyapp.student"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6411b1bb-3f42-4b55-a278-0994daa7d6d2"
      }
    }
  }
}
